<idea-plugin version="2">
  <name>SwungWeave</name>
  <description>SwungWeave: Bytecode weaver to simplify Swing UI code</description>
  <version>${version}</version>
  <vendor>RealityForge.org</vendor>
  <idea-version since-build="8000"/>

  <application-components>
  </application-components>

  <project-components>
    <component>
      <implementation-class>org.realityforge.swung_weave.idea.SwungWeaveProjectComponent</implementation-class>
    </component>
  </project-components>

  <actions>
  </actions>

  <extensions defaultExtensionNs="com.intellij">
    <facetType implementation="org.realityforge.swung_weave.facet.SwungWeaveFacetType"/>
    <frameworkSupport implementation="org.realityforge.swung_weave.facet.SwungWeaveSupportProvider"/>
  </extensions>
</idea-plugin>
